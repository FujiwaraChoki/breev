{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Breev",
  "version": "0.1.0",
  "identifier": "com.choki.breev",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Breev",
        "width": 720,
        "height": 800,
        "resizable": true,
        "minWidth": 400,
        "minHeight": 500
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQyRDBBMTAzNkFERDMwMzMKUldRek1OMXFBNkhRMGplWG5HQ1RuOW1zSm4wYlpXdnJVKzRYUUpob3hDeEJzckUvZ1VTcU5ZQ3QK",
      "endpoints": [
        "https://github.com/FujiwaraChoki/breev/releases/latest/download/latest.json"
      ]
    }
  }
}
